{
  "name": "madeitbelgium/laravel-vestacp",
  "version": "0.2.0",
  "description": "PHP Laravel VestaCP",
  "keywords": ["hosting", "vestacp"],
  "homepage": "http://www.madeit.be",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Tjebbe Lievens - Made I.T.",
      "homepage": "https://www.madeit.be",
      "email": "tjebbe.lievens@madeit.be"
    }
  ],
  "require": {
    "illuminate/support": "5.*|6.*|7.*|8.*",
    "illuminate/container": "5.*|6.*|7.*|8.*",
    "guzzlehttp/guzzle": "6.*|7.*",
    "illuminate/validation": "5.*|6.*|7.*|8.*",
    "php": ">=7.1"
  },
  "require-dev": {
      "phpunit/phpunit": "7.*|6.*|5.*|8.*|9.*",
      "mockery/mockery": "1.*",
      "php-coveralls/php-coveralls": "2.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
      "psr-4": {
          "MadeITBelgium\\VestaCP\\": "src/"
      }
  },
  "extra": {
       "laravel": {
           "providers": [
               "MadeITBelgium\\VestaCP\\VestaCPServiceProvider"
           ]
       }
    }
}
